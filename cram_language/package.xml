<package>
  <name>cram_language</name>
  <version>0.1.0</version>
  <description>
    The CRAM plan language is the core of the CRAM framework. It
    provides the basic functionality to write flexible and robust
    robot control programs.
  </description>

  <author email="moesenle@cs.tum.edu">Lorenz Moesenlechner</author>

  <maintainer email="moesenle@cs.tum.edu">Lorenz Moesenlechner</maintainer>
  <maintainer email="georg.bartels@cs.uni-bremen.de">Georg Bartels</maintainer>

  <license>BSD</license>

  <url type="website">http://ros.org/wiki/cram_language</url>
  <url type="bugtracker">https://github.com/cram-code/cram_core/issues</url>
  <url type="repository">https://github.com/cram-code/cram_core</url>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>sbcl</build_depend>
  <build_depend>alexandria</build_depend>
  <build_depend>trivial_garbage</build_depend>
  <build_depend>cram_utilities</build_depend>
  <build_depend>cram_test_utilities</build_depend>
  <build_depend>synchronization_tools</build_depend>

  <run_depend>sbcl</run_depend>
  <run_depend>alexandria</run_depend>
  <run_depend>trivial_garbage</run_depend>
  <run_depend>cram_utilities</run_depend>
  <run_depend>cram_test_utilities</run_depend>
  <run_depend>synchronization_tools</run_depend>
</package>


